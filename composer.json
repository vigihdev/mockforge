{
    "name": "vigihdev/mockforge",
    "description": "A tool for generating mock data for WordPress entities (posts, pages, custom post types, taxonomies, terms, users, comments) efficiently via the command line.",
    "homepage": "https://github.com/vigihdev/mockforge",
    "keywords": [
        "mockforge",
        "wordpress",
        "wordpress-cli",
        "command-line",
        "developer-tools",
        "content-management",
        "post-type",
        "term-management",
        "efficiency-tools"
    ],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Vigih Dev",
            "email": "vigihdev@gmail.com",
            "role": "developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.id"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/filesystem": "^6.4",
        "fakerphp/faker": "^1.23",
        "symfony/console": "^6.4",
        "symfony/dotenv": "^6.4",
        "vigihdev/serializer": "dev-main",
        "vigihdev/symfony-bridge-config": "dev-main",
        "vigihdev/support": "dev-main",
        "fightbulc/moment": "^1.35",
        "smknstd/fakerphp-picsum-images": "^1.0",
        "vigihdev/downloader": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "10.5.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Vigihdev\\MockForge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vigihdev\\MockForge\\Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-html coverage",
        "dev": "php -S localhost:3112 local-dev/index.php"
    }
}
