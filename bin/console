#!/usr/bin/env php
<?php
// console

require __DIR__ . '/../vendor/autoload.php';

use Symfony\Component\Console\Application;
use Vigihdev\MockForge\Command\Mock\Wp\{UserWpMockCommand, CategoryWpMockCommand, PostWpMockCommand};

// Application
$app = new Application();

// Block Mock Wp
$app->add(new UserWpMockCommand());
